{
   "name": "RassurBackend",
   "version": "0.0.1",
   "description": "Awesome project developed with TypeORM.",
   "type": "commonjs",
   "devDependencies": {
      "@types/express": "^4.17.17",
      "@types/node": "^16.11.10",
      "ts-node": "10.7.0",
      "typescript": "4.5.2"
      
   },
   "dependencies": {
      "body-parser": "^1.20.2",
      "cors": "^2.8.5",
      "express": "^4.18.2",      
      "express-fileupload": "^1.4.0",
      "helmet": "^7.0.0",
      "morgan": "^1.10.0",
      "mysql2": "^3.3.1",
      "nodemon": "^2.0.22",
      "reflect-metadata": "^0.1.13",
      "typeorm": "0.3.16"
   },
   "scripts": {
      "start": "nodemon app.ts",
      "typeorm": "typeorm-ts-node-commonjs",
      "typeorm:sync": "npm run typeorm schema:sync  -- -d src/app/database/data-source.ts",
      "migration:create" : "npm run typeorm migration:create --  src/app/database/data-source.ts",
      "migration:run" : "npm run typeorm migration:run -- -d src/app/database/data-source.ts",
      "migration:revert" : "npm run typeorm migration:revert -- -d src/app/database/data-source.ts",
      "migration:run:fake": "npm run typeorm migration:run --fake"
   }
}
